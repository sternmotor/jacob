    },
    "networkd": {},
    "passwd": {
        "users": [{
            "name": "core",
            "sshAuthorizedKeys": [
                "ssh-rsa  gunnar@macbook",
                "ssh-rsa  g.mann@quince",
                "ssh-rsa  root@sternmotor2"
            ]
        }]
    },
    "storage": {
        "files": [
            {
                "filesystem": "root",
                "path": "/etc/hostname",
                "mode": 420,
                "contents": {
                    "source": "data:,elvis"
                }
            }
        ],
        "links": [{
            "filesystem": "root",
            "path": "/etc/localtime",
            "target": "/usr/share/zoneinfo/Europe/Berlin",
            "overwrite": true
        }],
        "disks": [{
            "device": "/dev/sda",
            "partitions": [
                {
                    "label": "ROOT",
                    "number": 9,
                    "sizeMiB": 49152,
                    "startMiB": 0,
                    "typeGuid": "4F68BCE3-E8CD-4DB1-96E7-FBCAF984B709",
                    "wipePartitionEntry": true
                },
                {
                    "label": "DATA",
                    "number": 10,
                    "sizeMiB": 0,
                    "startMiB": 0,
                    "typeGuid": "3B8F8425-20E0-4F3B-907F-1A25A76F98E8"
                }
            ]
        }],
        "filesystems": [
            {
                "mount": {
                    "label": "DATA",
                    "device": "/dev/disk/by-partlabel/DATA",
                    "format": "btrfs",
                    "wipeFilesystem": true
                }
            }
        ]
    },
    "systemd": {
        "units": [{
            "contents": "[Mount]\nWhat=/dev/disk/by-label/DATA\nWhere=/srv\nType=btrfs\nOptions=defaults,noatime,compress=zstd\n[Install]\nWantedBy=local-fs.target",
            "enable": true,
            "name": "srv.mount"
        }]
    }
}
